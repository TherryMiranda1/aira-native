{
  "expo": {
    "name": "aira-native",
    "slug": "aira-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aira-logo.png",
    "scheme": "airanative",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/aira-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.airalabs.airanative"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/aira-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/aira-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf0c8120-295b-46cb-b698-d9a3cb050568"
      }
    }
  }
}
